    <bean id="ureport.kaiTiFontRegister" class="com.bstek.ureport.font.kaiti.KaiTiFontRegister"/>
    <bean id="ureport.songTiFontRegister" class="com.bstek.ureport.font.songti.SongTiFontRegister"/>
